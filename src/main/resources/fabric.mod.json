{
  "schemaVersion": 1,
  "id": "scaleddragonfight",
  "version": "${version}",
  "name": "ScaledDragonFight",
  "description": "A simple mod that scales the dragon fight according to the number of players in the end.",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/scaleddragonfight/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.zephbyte.scaleddragonfight.ScaledDragonFight"
    ]
  },
  "mixins": [
    "scaleddragonfight.mixins.json",
    {
      "config": "scaleddragonfight.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
